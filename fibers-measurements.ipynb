    "#define a custom class to read filename from cv.imread\n",
    "class MyImage:\n",
    "    def __init__(self, img_name):\n",
    "        self.img = cv.imread(img_name, cv.IMREAD_GRAYSCALE)\n",
    "        self.__name = img_name\n",
    "\n",
    "    def __str__(self):\n",
    "        return self.__name"
   ]
